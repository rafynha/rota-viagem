  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceRoot}/bin/Debug/netcoreapp3.1/Console.dll",
      "args": [
        "C:\\Users\\Desenvolvedor\\Documents\\TESTE BANCO BEXS\\input-routes.csv"
      ],
      "cwd": "${workspaceRoot}",
      "externalConsole": true,
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnFirstSessionStart"
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
